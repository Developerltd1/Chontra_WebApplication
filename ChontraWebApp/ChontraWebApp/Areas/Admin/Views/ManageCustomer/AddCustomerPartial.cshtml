    function showMessageBoxJquery(type, message) {
        if (type == "success") {
            $('#alert-Message-jquery').html("<div  class='btn btn-success' data-notify-type='success' data-notify-msg='<i class=icon-ok-sign></i> Message Sent Successfully!' onclick='SEMICOLON.widget.notifications({ el: this }); return false;>Show Success</div>");

           // $('#alert-Message-jquery').html("<div class='alert-success bg-success text-white  border-0 alert msg-box' role='alert'><strong><i class='mdi mdi-check-bold'></i></strong> " + message + " <button type='button' class='close' data-dismiss='alert' aria-label='Close'> <span aria-hidden='true'>×</span> </button></div>");
        }
        else {

            $('#alert-Message-jquery').html("<div class='alert-danger bg-danger text-white  border-0 alert msg-box' role='alert'><strong><i class='mdi mdi-check-bold'></i></strong> " + message + " <button type='button' class='close' data-dismiss='alert' aria-label='Close'> <span aria-hidden='true'>×</span> </button></div>");
        }
    }
</script>


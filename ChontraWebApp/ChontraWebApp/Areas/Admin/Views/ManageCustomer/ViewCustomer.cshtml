}

<div class="page-content">
    <div class="container-fluid">
        <div class="row mt-4">
            <div class="col-sm-12">
              
                <div class="card box-shadow">
                    <div class="card-header">
                       <!--Start_Modal-->
                         <button class="btn btn-sm float-sm-right btn-success  _click"><i class="mdi mdi-plus"></i> Create New </button>
                         <div class="modal fade" role="dialog" id="myModal" data-url='@Url.Action("AddCustomerPartial","ManageCustomer")'></div>
                       <!--End_Modal-->
                    </div>
                    <div class="card-body">
                        <div class="col-md-12">
                            <table id="tbl" class="table table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>Board / University Name</th>
                                        <th>Short Name</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                   <tr>
                                       <td>owNo</td>
                                       <td>BoardName</td>
                                       <td>ShortName</td>
                                   </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div> 

            </div>
        </div>
    </div>
</div>
